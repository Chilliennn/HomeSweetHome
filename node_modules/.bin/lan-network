#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chillien/code/HomeSweetHome/node_modules/lan-network/dist/node_modules:/Users/chillien/code/HomeSweetHome/node_modules/lan-network/node_modules:/Users/chillien/code/HomeSweetHome/node_modules:/Users/chillien/code/node_modules:/Users/chillien/node_modules:/Users/node_modules:/node_modules:/Users/chillien/code/HomeSweetHome/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chillien/code/HomeSweetHome/node_modules/lan-network/dist/node_modules:/Users/chillien/code/HomeSweetHome/node_modules/lan-network/node_modules:/Users/chillien/code/HomeSweetHome/node_modules:/Users/chillien/code/node_modules:/Users/chillien/node_modules:/Users/node_modules:/node_modules:/Users/chillien/code/HomeSweetHome/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lan-network/dist/lan-network-cli.js" "$@"
else
  exec node  "$basedir/../lan-network/dist/lan-network-cli.js" "$@"
fi
